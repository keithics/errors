{
  "name": "@signalyticsai/errors",
  "version": "1.0.3",
  "description": "Signalytics Error handing and Error codes",
  "main": "index.js",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx && prettier --check **/*.{js,ts} && tsc --noEmit",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/SignalyticsAI/errors.git"
  },
  "author": "keithics",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SignalyticsAI/errors/issues"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "homepage": "https://github.com/SignalyticsAI/errors#readme",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "prettier": "^2.3.0",
    "pretty-format": "^26.6.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.8",
    "tslint": "^6.1.3",
    "typescript": "4.4.4"
  },
  "dependencies": {
    "@sentry/node": "^6.17.2"
  }
}
